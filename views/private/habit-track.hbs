  <h2>Habits Track</h2>
  
  <div id="habit-list">
    <div class=habit-track"></div>
        <a href="/private/habit-dashboard"><button>Dashboard</button></a>
        <br> 
        <div><h2>"Description: {{description}}"</h2></div> 
        <div><h4>"AP name: {{ap.name}}"</h4></div>
        <div><h4>"Cue Day: {{cueDay}}"</h4></div>   
        <div><h4>"Cue DayTime: Mon:{{this.cueDayTime.Mon}} 
                            Tue:{{this.cueDayTime.Tue}} 
                            Wed:{{this.cueDayTime.Wed}} 
                            Thu:{{this.cueDayTime.Thu}} 
                            Fri:{{this.cueDayTime.Fri}} 
                            Sat:{{this.cueDayTime.Sat}} 
                            Sun:{{this.cueDayTime.Sun}} "</h4></div> 
        <div><h4><span>"Number of punches:" {{punch.length}}</span></h4></div>
        <br>
        {{#each this.punch}}
            <div><h2>"--- >> {{this}}"</h2></div>
        {{/each}}
        <br>
        <br>
        {{_id}}
        <a href="/private/habit-punch/{{_id}}"><button>Punch It Dude!</button></a>
        <a href="/private/habit-update/{{_id}}"><button>update habit</button></a>
      </div>
  </div>
